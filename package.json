  "validate-branch-name": {
    "pattern": "^(tasks|epics|bugs|subtasks)/([A-Z]{2,6}-)\\d{1,}(-\\w{2,}){1,}|(^projectConfig/\\w{2,}(-\\w{1,})?)",
    "errorMsg": "Некорректное наименование ветки. Примеры корректного наименования: 'tasks/SINV-1155-...' или 'projectConfig/some-configuration'"
  },
  "browserslist": {
    "production": [
