{
  "name": "@luxms/bi-resources",
  "version": "9.0.0",
  "description": "Resources editor",
  "scripts": {
    "pull": "node scripts/pull.js",
    "pull:dev": "node scripts/pull.js --env development",
    "pull:prod": "node scripts/pull.js --env production",
    "_push": "node scripts/push.js",
    "push": "yon build && yon _push",
    "push:dev": "rimraf dist_development  && yarn test && webpack --env development && node scripts/push.js --env development",
    "push:prod": "rimraf dist_production  && yarn test && webpack --env production  && node scripts/push.js --env production",
    "start": "node scripts/start.js",
    "check:ts": "tsc -p tsconfig.json --noEmit",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint-fix:ts": "eslint --fix \"**/*.{ts,tsx}\"",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint-fix:scss": "npx stylelint --fix \"**/*.scss\"",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "test": "jest --passWithNoTests",
    "build": "rimraf dist && webpack --env production",
    "build:dev": "rimraf dist_development && yarn test && webpack --env development",
    "build:prod": "rimraf dist_production && yarn test && webpack --env production",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,scss}'  --config ./.prettierrc",
    "prepare": "npx husky install && shx chmod +x .husky/*",
    "playwright": "playwright test --ui",
    "create-topic": "node scripts/create.js topic",
    "create-dashboard": "node scripts/create.js dashboard",
    "create-dashlet": "node scripts/create.js dashlet"
  },
  "workspaces": [
    "bi-internal"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@gito.luxms.com:luxmsbi/luxmsbi-web-resources.git"
  },
  "keywords": [
    "luxms",
    "bi",
    "library",
    "vizel",
    "reactjs"
  ],
  "author": "esix",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://gito.luxms.com/luxmsbi/luxmsbi-web-internal/-/issues"
  },
  "homepage": "https://github.com/luxms/bi-magic-resources",
  "optionalDependencies": {
    "node-expose-sspi": "^0.1.60"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "format-number-with-string": "^0.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.24.5",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/react": "^18.2.64",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
